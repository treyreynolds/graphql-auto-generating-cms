    "react",
    "react cms",
    "graphql",
    "graphql cms",
    "keystone",
    "feathers",
    "auto generated",
    "auto cms",
    "React",
    "GraphQL CMS"
  ],
  "author": {
    "name": "Sarkis Arutiunian (sarkis.tlt@gmail.com)"
